	<!-- XM16E1 FPW -->
	<RecipeDef ParentName="RangedWeaponRecipeBase">
		<defName>BuildXM16E1FPW</defName>
		<label>Build XM16E1 FPW</label>
		<description>Builds a XM16E1 FPW. Totally not The Patriot from certain game...\nCaliber: 5.56x45mm NATO</description>
		<jobString>Building XM16E1 FPW.</jobString>
		<workAmount>13800</workAmount>
		<skillRequirements>
			<Crafting>14</Crafting>
		</skillRequirements>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>AdvRifle_Component</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Weapon_Parts</li>
					</thingDefs>
				</filter>
				<count>6</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentIndustrial</li>
					</thingDefs>
				</filter>
				<count>8</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>Metallic</li>
					</categories>
				</filter>
				<count>35</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Plastic</li>
					</thingDefs>
				</filter>
				<count>25</count>
			</li>
		</ingredients>
		<products>
			<Pa_XM16E1FPW>1</Pa_XM16E1FPW>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>AdvRifle_Component</li>
				<li>Weapon_Parts</li>
				<li>ComponentIndustrial</li>
				<li>Plastic</li>
			</thingDefs>
			<categories>
				<li>SLDBar</li>
				<li>USLDBar</li>
			</categories>
		</fixedIngredientFilter>
		<recipeUsers>
			<li>AdvWeaponCraftingWorkTable</li>
		</recipeUsers>
	    <researchPrerequisite>Rifles_C2</researchPrerequisite>
	</RecipeDef>
	
