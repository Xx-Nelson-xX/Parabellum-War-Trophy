	<!--4.5" Barrel-->
	<ThingDef ParentName="Module_Barrel">
		<defName>Module_5inchG3B</defName>
		<label>4.5" pistol barrel</label>
		<description>4.5 inch pistol length barrel of G3 family.</description>
		<statBases>
			<WorkToMake>260</WorkToMake>
			<Mass>0.177</Mass>
			<Bulk>0.14</Bulk>
			<MarketValue>50</MarketValue>
		</statBases>
		<costList>
			<Weapon_Parts>1</Weapon_Parts>
		</costList>
		<recipeMaker>
			<researchPrerequisite>BlowbackOperation</researchPrerequisite>
			<skillRequirements>
				<Crafting>2</Crafting>
			</skillRequirements>
		</recipeMaker>
		<comps>
			<li Class="RW_ModularizationWeapon.CompProperties_ModularizationWeapon">
				<PartTexPath>Things/WeaponPart/Barrels/PaWT_HKRB_Ba</PartTexPath>
				<drawChildPartWhenOnGround>false</drawChildPartWhenOnGround>
				<setRandomPartWhenCreate>true</setRandomPartWhenCreate>
				<statOffset>
					<Bulk>0.14</Bulk>
					<MarketValue>50</MarketValue>
				</statOffset>
				<verbPropertiesOffseter>
					<li>
						<range>6</range>
						<burstShotCount>6</burstShotCount>
						<ticksBetweenBurstShots>-1</ticksBetweenBurstShots>
						<muzzleFlashScale>2</muzzleFlashScale>
					</li>
				</verbPropertiesOffseter>
				<verbPropertiesMultiplier>
					<li>
						<warmupTime>0.95</warmupTime>
					</li>
				</verbPropertiesMultiplier>
				<statMultiplier>
					<SightsEfficiency>1.1</SightsEfficiency>
					<ShotSpread>0.85</ShotSpread>
					<RangedWeapon_Cooldown>0.9</RangedWeapon_Cooldown>
				</statMultiplier>
				<compPropertiesOffseter>
					<il Reader-Class="CombatExtended.CompProperties_FireModes">
						<aimedBurstShotCount>4</aimedBurstShotCount>
					</il>
				</compPropertiesOffseter>
				<attachmentProperties>
					<li>
						<id>ModAR_HG</id>
						<name>handguard</name>
						<UITexPath>ui/icons/AttachmentIcon/AttachmentIcon_Handguard</UITexPath>
						<postion>(0,0.1,0)</postion>
						<scale>(0.8,0,0.8)</scale>
						<defultThing>ModuleG3_MP5KHG</defultThing>
						<allowEmpty>true</allowEmpty>
						<filter>
							<thingDefs>
								<li>ModuleG3_MP5KHG</li>
							</thingDefs>
						</filter>
					</li>
					<li>
						<id>ModMP_Muzzle</id>
						<name>muzzle</name>
						<UITexPath>ui/icons/AttachmentIcon/AttachmentIcon_Muzzle</UITexPath>
						<postion>(-0.105,0.1,0)</postion>
						<scale>(0.5,0,0.5)</scale>
						<allowEmpty>true</allowEmpty>
						<filter>
							<thingDefs>
								<li>Module_Silencer</li>
								<li>Module_MuzzleBrake</li>
								<li>Module_MuzzleCompensator</li>
								<li>Module_FlashHider</li>
							</thingDefs>
						</filter>
					</li>
				</attachmentProperties>
			</li>
		</comps>
	</ThingDef>
	
